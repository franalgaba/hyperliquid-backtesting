          "outputs": ["value"]
        },
        {
          "id": "sma_10",
          "type": "SMA",
          "params": {
            "length": 10.0
          },
          "outputs": ["value"]
        }
      ],
      "graph": {
        "entry": "check_momentum",
        "nodes": {
          "check_momentum": {
            "type": "condition",
            "expr": {
              "op": ">",
