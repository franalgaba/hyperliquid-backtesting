              },
              "order": {
                "type": "MARKET"
              }
            },
            "guards": [],
            "next": "end"
          },
          "sell_signal": {
            "type": "action",
            "action": {
              "kind": "SELL",
              "symbol": "BTCUSDC",
              "sizing": {
                "mode": "pct",
                "value": 100.0
              },
              "order": {
                "type": "MARKET"
              }
            },
            "guards": [],
            "next": "end"
          },
          "end": {
            "type": "terminal"
          }
        }
      }
    }
